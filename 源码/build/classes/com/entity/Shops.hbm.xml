<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.entity.Shops" table="T_Shops" catalog="projectdb">
		<id name="Susername" type="java.lang.String">
			<column name="Susername" length="20"/>
			<generator class="assigned"/> 
		</id>
		<property name="Sname" type="java.lang.String">
			<column name="Sname" length="20" not-null="true"/>
		</property>
		<property name="Spassword" type="java.lang.String">
			<column name="Spassword" length="20" default="123456"/>
		</property>
		<property name="Ssex" type="java.lang.String">
			<column name="Ssex" length="2" not-null="true"/>
		</property>
		<property name="Sphone" type="java.lang.String">
			<column name="Sphone" length="11" not-null="true"/>
		</property>
		<property name="Sshopname" type="java.lang.String">
			<column name="Sshopname" length="50" not-null="true"/>
		</property>
		<property name="Slocal" type="java.lang.String">
			<column name="Slocal" length="50" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>